package main;

public class Book {
	private final String title;
	private final String author;
	private final String subject;
	private final long ISBN;
	private final int pages;
	
	public Book(String title, String author, String subject, long ISBN, int pages) {
		this.title = title;
		this.author = author;
		this.subject = subject;
		this.ISBN = ISBN;
		this.pages = pages;
	}
}
