package main;

import java.io.File;
import java.io.FileNotFoundException;

public class Main {
	public static void main(String[] args) {
		Library myLib;
		try {
			myLib = new Library(new File("books.txt"));
			myLib.listBooks(1);
			System.out.println(myLib.getBook(myLib.getBook("Percy Jackson")).getAuthor());
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
	}
}
