package main;

import java.io.File;
import java.io.FileNotFoundException;

public class Main {
	public static void main(String[] args) {
		Library myLib;
		try {
			myLib = new Library(new File("books.txt"));
			myLib.listBooks(1);
			Book b = myLib.getBook("Percy Jackson");
			Book otherB = new Book(b);
			myLib.removeBook(b);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
	}
}
