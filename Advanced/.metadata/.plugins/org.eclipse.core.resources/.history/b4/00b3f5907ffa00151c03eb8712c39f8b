package main;

import java.io.File;
import java.io.FileNotFoundException;

public class Main {
	public static void main(String[] args) {
		Library myLib;
		try {
			myLib = new Library(new File("books.txt"));
			myLib.listBooks(5);
			Book b = myLib.getBook("there ya go");
			System.out.println(b.getISBN());
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
	}
}
